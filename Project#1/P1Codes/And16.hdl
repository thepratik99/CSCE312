/**
 * 16-bit bitwise And:
 * for i = 0..15: out[i] = (a[i] and b[i])
 */

//Name: Pratik Patel
//UIN: 527004337

CHIP And16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    And8(a[0..7]=a[0..7], b[0..7]=b[0..7], out[0..7]=out[0..7]);
    And8(a[0..7]=a[8..15], b[0..7]=b[8..15], out[0..7]=out[8..15]);
}